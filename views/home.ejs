<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Chat Demo</title>
        <link rel="stylesheet" href="home.css" />
    </head>
    <body>
        <div id="mario-chat">
            <h2>Chat</h2>
            <div id="chat-window">
                <div id="output"></div>
                <div id="feedback"></div>
            </div>
            <div>
                <legend>Channel</legend>

                <input type="radio" name="channels" id="channel1" value="1" checked />
                <label for="channel1"> 1</label>

                <input type="radio" name="channels" id="channel2" value="2" />
                <label for="channel2"> 2</label>

                <input type="radio" name="channels" id="channel3" value="3" />
                <label for="channel1"> 3</label>
            </div>
            <input id="user" type="text" placeholder="User" />
            <input id="message" type="text" placeholder="Message" />
            <button id="send">Send</button>
        </div>
    </body>

    <!-- <script src="/socket.io/socket.io.js"></script> -->
    <script
        src="https://cdn.socket.io/4.7.4/socket.io.min.js"
        integrity="sha384-Gr6Lu2Ajx28mzwyVR8CFkULdCU7kMlZ9UthllibdOSo6qAiN+yXNHqtgdTvFXMT4"
        crossorigin="anonymous"
    ></script>
    <script src="app.js"></script>
</html>
